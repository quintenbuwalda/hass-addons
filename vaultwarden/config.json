{
  "name": "Vaultwarden+",
  "version": "1.34.1",
  "slug": "vaultwarden",
  "description": "Alternative implementation of the Bitwarden server API.",
  "arch": [
    "amd64",
    "aarch64"
  ],
  "homeassistant": "2021.9.0",
  "startup": "services",
  "boot": "auto",
  "init": true,
  "run": "/run.sh",
  "image": "ghcr.io/dani-garcia/vaultwarden",
  "ports": {
    "80/tcp": 7277
  },
  "ports_description": {
    "80/tcp": "Web interface"
  },
  "map": [
    "addon_config:rw"
  ],
  "environment": {
    "DATA_FOLDER": "/config"
  },
  "options": {
    "push_enabled": false,
    "push_relay_uri": "",
    "push_identity_uri": "",
    "push_installation_id": "",
    "push_installation_key": ""
  },
  "schema": {
    "push_enabled": "bool",
    "push_relay_uri": "str?",
    "push_identity_uri": "str?",
    "push_installation_id": "str?",
    "push_installation_key": "str?"
  },
  "ingress": true,
  "ingress_port": 80,
  "webui": "[PROTO:ssl]://[HOST]:[PORT:80]"
}
